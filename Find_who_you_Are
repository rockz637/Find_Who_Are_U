public class WhoRu {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the first letter of your name ");
		char input=scan.next().charAt(0);
		switch(input) {
		case 'a' :
		case 'A' :
			Words.Auser();
			break;
		case 'b' :
		case 'B' :
			Words.Buser();
			break;
		case 'c' :
		case 'C' :
			Words.Cuser();
			break;
		case 'd' :
		case 'D' :
			Words.Duser();
			break;
		case 'e' :
		case 'E' :
			Words.Euser();
			break;
		case 'f' :
		case 'F' :
			Words.Fuser();
			break;
		case 'g' :
		case 'G' :
			Words.Guser();
			break;
		case 'h' :
		case 'H' :
			Words.Huser();
			break;
		case 'i' :
		case 'I' :
			Words.Iuser();
			break;
		case 'j' :
		case 'J' :
			Words.Juser();
			break;
		case 'k' :
		case 'K' :
			Words.Kuser();
			break;
		case 'l' :
		case 'L' :
			Words.Luser();
			break;
		case 'm' :
		case 'M' :
			Words.Muser();
			break;
		case 'n' :
		case 'N' :
			Words.Nuser();
			break;
		case 'o' :
		case 'O' :
			Words.Ouser();
			break;
		case 'p' :
		case 'P' :
			Words.Puser();
			break;
		case 'q' :
		case 'Q' :
			Words.Quser();
			break;
		case 'r' :
		case 'R' :
			Words.Ruser();
		case 's' :
		case 'S' :
			Words.Suser();
			break;
		case 't' :
		case 'T' :
			Words.Buser();
			break;
		case 'u' :
		case 'U' :
			Words.Uuser();
			break;
